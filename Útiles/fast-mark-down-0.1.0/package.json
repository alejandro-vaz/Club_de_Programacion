{
    "engines": {
        "vscode": "^1.93.1"
    },
    "name": "fastmarkdown",
    "displayName": "FastMarkDown",
    "description": "FastMarkDown is a custom language designed to enhance your coding experience in Markdown, enabling faster and more efficient writing with syntax highlighting and other features.",
    "version": "0.1.0",
    "publisher": "Alejandro_Vaz",
    "contributes": {
        "languages": [{
            "id": "fmd",
            "extensions": [".fmd"],
            "aliases": ["FastMarkDown"]
        }],
        "grammars": [{
            "language": "fmd",
            "scopeName": "source.fmd",
            "path": "./syntaxes/fastmarkdown.tmLanguage.json"
        }],
        "keywords": [
            "markdown",
            "fastmarkdown",
            "syntax highlighting",
            "programming",
            "editor"
        ]
    },
    "license": "MIT"
}